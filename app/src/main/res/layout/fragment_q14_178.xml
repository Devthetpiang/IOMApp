<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical" android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:padding="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/q_q14"
            android:text="Q14. ေအာက္ပါက်န္းမာေရးဝန္ေဆာင္မႈမ်ားသည္ ေက်းရြာ/ရပ္ကြက္ထဲတြင္ ရရွိႏိုင္ပါသလား။"/>


        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:orientation="horizontal"
            android:layout_below="@id/q_q14"
            android:rowCount="16"
            android:columnCount="3"
            >

            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="0"
                android:text=""/>
            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="1"
                android:text="ရွိပါသည္။"/>
            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="2"
                android:text="မရွိပါ။"/>

            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_row="1"
                android:layout_column="0"
                android:text="Q14a. ေဆးရံု"/>
            <RadioGroup
                android:id="@+id/rg_q14a"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_row="1"
                android:layout_columnSpan="2"
                >
                <com.xavey.proto.control.ProtoRadioButton
                    android:id="@+id/rg_q14a_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="60dp"/>
                <com.xavey.proto.control.ProtoRadioButton
                    android:id="@+id/rg_q14a_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="60dp"/>
            </RadioGroup>

            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_column="0"
                android:text="Q14b. ေက်းလက္ေဆးခန္း (သူနာျပဳမ်ားရွိသည္။)"/>
            <RadioGroup
                android:id="@+id/rg_q14b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_row="2"
                android:layout_columnSpan="2"
                >
                <com.xavey.proto.control.ProtoRadioButton
                    android:id="@+id/rg_q14b_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="60dp"/>
                <com.xavey.proto.control.ProtoRadioButton
                    android:id="@+id/rg_q14b_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="60dp"/>
            </RadioGroup>

            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_row="3"
                android:layout_column="0"
                android:text="Q14c. ေက်းလက္ေဆးခန္းအခြဲ (သားဖြားဆရာမမ်ားရွိသည္။)"/>
            <RadioGroup
                android:id="@+id/rg_q14c"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_row="3"
                android:layout_columnSpan="2"
                >
                <com.xavey.proto.control.ProtoRadioButton
                    android:id="@+id/rg_q14c_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="60dp"/>
                <com.xavey.proto.control.ProtoRadioButton
                    android:id="@+id/rg_q14c_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="60dp"/>
            </RadioGroup>

            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_row="4"
                android:layout_column="0"
                android:text="Q14d. ေရြ႕လ်ားေဆးခန္း"/>
            <RadioGroup
                android:id="@+id/rg_q14d"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_row="4"
                android:layout_columnSpan="2"
                >
                <com.xavey.proto.control.ProtoRadioButton
                    android:id="@+id/rg_q14d_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="60dp"/>
                <com.xavey.proto.control.ProtoRadioButton
                    android:id="@+id/rg_q14d_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="60dp"/>
            </RadioGroup>

            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_row="5"
                android:layout_column="0"
                android:text="Q14e. ပုဂၢလိက ေဆးခန္း"/>
            <RadioGroup
                android:id="@+id/rg_q14e"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_row="5"
                android:layout_columnSpan="2"
                >
                <com.xavey.proto.control.ProtoRadioButton
                    android:id="@+id/rg_q14e_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="60dp"/>
                <com.xavey.proto.control.ProtoRadioButton
                    android:id="@+id/rg_q14e_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="60dp"/>
            </RadioGroup>

            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_row="6"
                android:layout_column="0"
                android:text="Q14f. ရပ္ရြာအတြင္း က်န္းမာေရးလုပ္သားမ်ား"/>
            <RadioGroup
                android:id="@+id/rg_q14f"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_row="6"
                android:layout_columnSpan="2"
                >
                <com.xavey.proto.control.ProtoRadioButton
                    android:id="@+id/rg_q14f_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="60dp"/>
                <com.xavey.proto.control.ProtoRadioButton
                    android:id="@+id/rg_q14f_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="60dp"/>
            </RadioGroup>

            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_row="7"
                android:layout_column="0"
                android:text="Q14g. အဂၤလိပ္ေဆးေရာင္းသူ"/>
            <RadioGroup
                android:id="@+id/rg_q14g"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_row="7"
                android:layout_columnSpan="2"
                >
                <com.xavey.proto.control.ProtoRadioButton
                    android:id="@+id/rg_q14g_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="60dp"/>
                <com.xavey.proto.control.ProtoRadioButton
                    android:id="@+id/rg_q14g_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="60dp"/>
            </RadioGroup>

            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_row="8"
                android:layout_column="0"
                android:text="Q14h. တိုင္းရင္းေဆးေရာင္းသူ"/>
            <RadioGroup
                android:id="@+id/rg_q14h"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_row="8"
                android:layout_columnSpan="2"
                >
                <com.xavey.proto.control.ProtoRadioButton
                    android:id="@+id/rg_q14h_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="60dp"/>
                <com.xavey.proto.control.ProtoRadioButton
                    android:id="@+id/rg_q14h_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="60dp"/>
            </RadioGroup>
        </GridLayout>
    </RelativeLayout>
</ScrollView>