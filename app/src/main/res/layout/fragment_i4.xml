<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:padding="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/q_i4"
        android:text="I4. ၿပီးခဲ့သည့္လအတြင္းတြင္ ေအာက္ပါတို႔မွ မည္သည္တို႔ ခံစားရ"/>


    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="20dp"
        android:orientation="horizontal"
        android:layout_below="@id/q_i4"
        android:rowCount="16"
        android:columnCount="3"
        >

        <TextView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="0"
            android:text=""/>
        <TextView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="1"
            android:text="ခံစားရပါသည္"/>
        <TextView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="2"
            android:text="မခံစားရပါ"/>

        <TextView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_column="0"
            android:text="1 ေခါင္းကိုက္ျခင္း"/>

        <RadioGroup
            android:id="@+id/rg_i4_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_row="1"
            android:layout_columnSpan="2"
            >
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_i4_1_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="20dp"/>

            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_i4_1_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RadioGroup>
        <TextView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="0"
            android:text="2 အဆစ္အျမစ္ကိုက္ျခင္း"/>

        <RadioGroup
            android:id="@+id/rg_i4_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_row="2"
            android:layout_columnSpan="2"
            >
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_i4_2_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="20dp"/>

            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_i4_2_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RadioGroup>
        <TextView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_row="3"
            android:layout_column="0"
            android:text="3 ခါး သို႔မဟုတ္ တင္ နာက်င္ျခင္း"/>

        <RadioGroup
            android:id="@+id/rg_i4_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_row="3"
            android:layout_columnSpan="2"
            >
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_i4_3_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="20dp"/>

            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_i4_3_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RadioGroup>
        <TextView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_row="4"
            android:layout_column="0"
            android:text="4 အားနည္းျခင္း"/>

        <RadioGroup
            android:id="@+id/rg_i4_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_row="4"
            android:layout_columnSpan="2"
            >
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_i4_4_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="20dp"/>

            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_i4_4_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RadioGroup>

    </GridLayout>


</RelativeLayout>
</ScrollView>