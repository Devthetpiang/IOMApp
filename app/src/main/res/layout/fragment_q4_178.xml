<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical" android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:padding="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/q_q4"
            android:text="Q4. ေက်းရြာ/ရပ္ကြက္ထဲတြင္ေနထိုင္ေသာသူမ်ား အဓိကလုပ္ကိုင္စားေသာက္ေသာ အလုပ္သံုးမ်ိဳးကို ေျပာျပပါ။ [ေမးျမန္းသူအတြက္ ညႊန္ၾကားခ်က္: အဓိကအလုပ္သံုးမ်ိဳးကို မွတ္သားပါ။ အမ်ားဆံုးလုပ္ေသာအလုပ္မွ စတင္၍ မွတ္ပါ။] "/>

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:orientation="horizontal"
            android:layout_below="@id/q_q4"
            android:rowCount="16"
            android:columnCount="3"
            >

            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="0"
                android:text=""/>
            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="1"
                android:text="Code"/>
            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="2"
                android:text="အျခား"/>

            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_row="1"
                android:layout_column="0"
                android:text="Q4a. ပထမ"/>
            <com.xavey.proto.control.ProtoSpinner
                android:id="@+id/sp_q4a"
                android:layout_row="1"
                android:layout_column="1"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:entries="@array/q4" />
            <com.xavey.proto.control.ProtoEditText
                android:id="@+id/et_q4a_other"
                android:layout_row="1"
                android:layout_column="2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="ေဖာ္ျပပါ" />

            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_column="0"
                android:text="Q4b. ဒုတိယ"/>
            <com.xavey.proto.control.ProtoSpinner
                android:id="@+id/sp_q4b"
                android:layout_row="2"
                android:layout_column="1"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:entries="@array/q4" />
            <com.xavey.proto.control.ProtoEditText
                android:id="@+id/et_q4b_other"
                android:layout_row="2"
                android:layout_column="2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="ေဖာ္ျပပါ" />

            <TextView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_row="3"
                android:layout_column="0"
                android:text="Q4c. တတိယ"/>
            <com.xavey.proto.control.ProtoSpinner
                android:id="@+id/sp_q4c"
                android:layout_row="3"
                android:layout_column="1"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:entries="@array/q4" />
            <com.xavey.proto.control.ProtoEditText
                android:id="@+id/et_q4c_other"
                android:layout_row="3"
                android:layout_column="2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="ေဖာ္ျပပါ" />
        </GridLayout>
    </RelativeLayout>
</ScrollView>