<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:padding="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/q_f8"
        android:text="ေျမး(မ်ား)ကို ေစာင့္ေရွာက္ေနသည့္ အေတာအတြင္း ေအာက္ပါတို႔ထံမွ အေထာက္အပံ့မ်ားကို ရရွိပါသလား။ [ေမးျမန္းသူ: တစ္ခုခ်င္းစီေမးပါ။] "/>


    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="20dp"
        android:orientation="horizontal"
        android:layout_below="@id/q_f8"
        android:rowCount="16"
        android:columnCount="4"
        >

        <TextView
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="0"
            android:text=""/>
        <TextView
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="1"
            android:text="ရပါသည္။"/>
        <TextView
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="2"
            android:text="မရပါ။"/>

        <TextView
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="3"
            android:text="မေသခ်ာပါ။"/>

        <TextView
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_column="0"
            android:text="တစ္အိမ္ထဲအတူေနေသာ မိသားစုဝင္မ်ား"/>
        <RadioGroup
            android:id="@+id/rg_f8_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_row="1"
            android:layout_column="1"
            android:layout_columnSpan="3"
            >
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_f8_1_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="20dp"/>

            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_f8_1_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="20dp"/>
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_f8_1_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RadioGroup>
        <TextView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="0"
            android:text="အနီးအနားတြင္ရွိေသာ မိသားစုဝင္မ်ား"/>

        <RadioGroup
            android:id="@+id/rg_f8_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_row="2"
            android:layout_column="1"
            android:layout_columnSpan="3"
            >
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_f8_2_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="20dp"/>

            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_f8_2_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="20dp"/>
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_f8_2_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RadioGroup>
        <TextView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_row="3"
            android:layout_column="0"
            android:text="အိမ္နီးခ်င္းမ်ား/ သူငယ္ခ်င္းမ်ား"/>

        <RadioGroup
            android:id="@+id/rg_f8_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_row="3"
            android:layout_column="1"
            android:layout_columnSpan="3"
            >
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_f8_3_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="20dp"/>
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_f8_3_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="20dp"/>
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_f8_3_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RadioGroup>
        <TextView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_row="4"
            android:layout_column="0"
            android:text="အစိုးရမွ ဖြင့္လွစ္ထားေသာ ေန႔ကေလးထိန္းစင္တာ"/>

        <RadioGroup
            android:id="@+id/rg_f8_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_row="4"
            android:layout_column="1"
            android:layout_columnSpan="3"
            >
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_f8_4_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="20dp"/>
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_f8_4_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="20dp"/>
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_f8_4_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RadioGroup>
        <TextView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_row="5"
            android:layout_column="0"
            android:text="ပုဂၢလိက ေန႔ကေလးထိန္းစင္တာ"/>

        <RadioGroup
            android:id="@+id/rg_f8_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_row="5"
            android:layout_column="1"
            android:layout_columnSpan="3"
            >
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_f8_5_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="20dp"/>
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_f8_5_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="20dp"/>
            <com.xavey.proto.control.ProtoRadioButton
                android:id="@+id/rg_f8_5_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RadioGroup>
    </GridLayout>


</RelativeLayout>
</ScrollView>